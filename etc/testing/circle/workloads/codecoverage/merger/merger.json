{
  "pipeline": {
    "name": "codecoverage-merger"
  },
  "transform": {
    "image": "pachyderm/codecoverage:local",
    "cmd": [
      "/go/merger/merger",
      "/pfs/codecoverage-extractor",
      "/pfs/out"
    ],
    "env": {
      "LOG_LEVEL": "DEBUG"
    }
  },
  "input": {
    "pfs": {
      "repo": "codecoverage-extractor",
      "glob": "/*"
    }
  }
}