{
    "pipeline": {
      "name": "codecoverage-extractor"
    },
    "transform": {
      "image": "pachyderm/codecoverage:local",
      "cmd": ["/go/extractor/extractor", "/pfs/codecoverage-source", "/pfs/out"],
      "env": {
        "LOG_LEVEL": "DEBUG"
      }
    },
    "input": {
          "pfs": {
              "repo": "codecoverage-source",
              "glob": "/*"
          }
      }
  }
  