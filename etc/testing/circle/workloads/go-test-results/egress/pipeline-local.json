{
    "pipeline": {
        "name": "go-test-results-sql-egress"
    },
    "input": {
        "pfs": {
            "repo": "go-test-results-raw",
            "project": "ci-metrics",
            "glob": "/*/*"
        }
    },
    "transform": {
        "image": "pachyderm/go-test-results:0.0.1",
        "cmd": [
            "/go/egress/egress",
            "/pfs/go-test-results-raw"
        ],
        "env": {
            "LOG_LEVEL": "DEBUG",
            "POSTGRESQL_HOST": "postgres",
            "POSTGRESQL_USER":"pachyderm"
        },
        "secrets": [{
            "name": "postgres",
            "env_var": "POSTGRESQL_PASSWORD",
            "key": "postgresql-password"
        }]
    }
}